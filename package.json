  },
  "homepage": "https://github.com/Zhouzi/gulp-bucket",
  "devDependencies": {
    "assert": "*",
    "gulp": "*",
    "gulp-concat": "*",
    "gulp-uglify": "*",
    "mocha": "*",
    "pre-commit": "*",
    "sinon": "*",
    "standard": "*"
