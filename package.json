{
  "name": "gulp-bucket",
  "version": "0.0.5",
  "description": "Maximize gulp tasks reusability.",
  "keywords": [
    "gulpplugin",
    "task",
    "reusable",
    "module"
  ],
  "main": "dist/index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "tests",
    "testPathDirs": [
      "tests"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ]
  },
  "scripts": {
    "test": "jest tests/index.js",
    "gulp": "gulp --gulpfile tests/gulpfile.babel.js help && gulp --gulpfile tests/gulpfile.babel.js",
    "lint": "standard",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test",
      "gulp"
    ]
  },
  "author": "Gabin Aureche <hello@gabinaureche.com> (http://gabinaureche.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhouzi/gulp-bucket.git"
  },
  "bugs": {
    "url": "https://github.com/Zhouzi/gulp-bucket/issues"
  },
  "homepage": "https://github.com/Zhouzi/gulp-bucket",
  "devDependencies": {
    "babel-cli": "6.2.0",
    "babel-core": "6.2.1",
    "babel-jest": "6.0.1",
    "babel-preset-es2015": "6.1.18",
    "gulp": "3.9.0",
    "gulp-babel": "6.1.1",
    "gulp-concat": "2.6.0",
    "gulp-uglify": "1.5.1",
    "jest-cli": "0.8.0",
    "pre-commit": "1.1.2",
    "standard": "5.4.1"
  },
  "dependencies": {
    "colors": "1.1.2",
    "lodash": "3.10.1"
  }
}
